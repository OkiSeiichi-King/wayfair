":{"title":"Wayfair's OSPO: Auxiliary Engineering"},"parent":{"relativeDirectory":"ospo"}}},{"node":{"id":"fd0f1002-abc9-596f-9111-ba66c261762c","fields":{"slug":"/docs/ospo/platform/"},"frontmatter":{"title":"Wayfair's OSPO: Platform"},"parent":{"relativeDirectory":"ospo"}}}]}},"pageContext":{"slug":"/docs/"}},"staticQueryHashes":["3649515864","3983085624"]}